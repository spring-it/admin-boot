    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        status,
        deleted,
        create_time,
        create_by,
        update_time,
        update_by,
        id, account, nick_name, real_name, user_type, email, mobile, sex, avatar, birthday, password, remark
